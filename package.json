{
  "name": "zooid-gene-lab",
  "version": "1.0.0",
  "description": "GeneLab",
  "main": "dist/bundle.js",
  "scripts": {
    "build": "webpack",
    "build:storybook": "build-storybook -o storybook-build",
    "clean": "rimraf lib dist coverage",
    "coverage": "nyc npm test",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:fix": "npm run lint:js:fix && npm run lint:css:fix",
    "lint:js": "eslint src stories",
    "lint:js:fix": "eslint src stories --fix",
    "lint:css": "stylelint ./src/**/*.css",
    "lint:css:fix": "stylefmt ./src/**/*.css",
    "mocha:json": "mocha --reporter=json > coverage/mocha.json",
    "start": "npm run storybook",
    "storybook": "start-storybook -p 9001",
    "test": "mocha",
    "test:watch": "mocha -w -R mocha-multi --reporter-options spec=-,mocha-osx-reporter=-"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ringmath/zooid-gene-lab.git"
  },
  "keywords": [
    "zooid",
    "zooid-ui",
    "react",
    "reactjs",
    "octoblu"
  ],
  "author": "ringmath",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ringmath/zooid-gene-lab/issues"
  },
  "homepage": "https://github.com/ringmath/zooid-gene-lab#readme",
  "nyc": {
    "include": [
      "src"
    ],
    "exclude": [
      "src/**/*.spec.js",
      "src/**/*.css"
    ],
    "cache": true,
    "reporter": [
      "text-summary",
      "lcov",
      "json"
    ],
    "require": [
      "babel-core/register",
      "babel-polyfill"
    ]
  },
  "devDependencies": {
    "@kadira/react-storybook-addon-info": "^3.2.1",
    "@kadira/storybook": "^2.1.1",
    "babel-cli": "^6.7.7",
    "babel-core": "^6.7.7",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.5.0",
    "css-loader": "^0.23.1",
    "css-modules-require-hook": "^4.0.1",
    "env-test": "^1.0.0",
    "enzyme": "^2.3.0",
    "eslint": "^4.18.2",
    "eslint-plugin-import": "^1.12.0",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^6.0.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "ignore-styles": "^4.0.0",
    "jsdom": "^9.1.0",
    "mocha": "^3.0.0",
    "mocha-multi": "^0.9.1",
    "mocha-osx-reporter": "^0.1.2",
    "nyc": "^8.1.0",
    "postcss-loader": "^0.9.1",
    "react-addons-test-utils": "^15.3.0",
    "rimraf": "^2.5.2",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.1",
    "stylefmt": "^4.2.3",
    "stylelint": "^7.1.0",
    "stylelint-config-standard": "^12.0.0",
    "webpack": "^1.13.0"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "react": "^15.3.0",
    "react-dom": "^15.3.0"
  }
}
